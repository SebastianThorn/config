  ui = true
[alias]
  st = status -sb
  ci = commit
  co = checkout
  sl = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[color "status"]
  added = green bold
  changed = red bold
  untracked = cyan
  branch = yellow black bold ul
